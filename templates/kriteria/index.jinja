<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="{{ url_for('static', filename='css/style.css') }}" rel="stylesheet" />
    <title>Document</title>
  </head>
  <body>
    <div class="">
      <a href="/ahli">ahli</a>
      <a href="/swara">swara</a>
      <a href="/tambah">tambah</a>
      <a href="/bobot">bobot</a>
      <a href="/alternatif">alternatif</a>
      <table class="text-center">
        <tr>
          <th class="">No</th>
          <th>Kriteria</th>
          <th>Jenis Kriteria</th>
          <th>aksi</th>
        </tr>
        {% for row in data %}
          <tr>
            <td>{{ loop.index0 + 1 }}</td>
            <td>{{ row.kriteria }}</td>
            <td>{{ row.jenis_kriteria }}</td>
            <td>
              <a href=" {{ url_for('edit', id=loop.index0) }} ">edit</a>
              <a href="{{ url_for('hapus', id=loop.index0) }}">hapus</a>
            </td>
          </tr>
        {% endfor %}
      </table>
    </div>
  </body>
</html>
